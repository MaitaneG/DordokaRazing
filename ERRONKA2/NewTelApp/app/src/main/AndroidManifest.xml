            android:label="@string/app_name"
            android:theme="@style/Theme.NewTelApp.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ProduktuakErakutsi"
            android:theme="@style/Theme.NewTelApp.NoActionBar">
        </activity>
        <activity
            android:name=".AurrekontuaSortu"
            android:theme="@style/Theme.NewTelApp.NoActionBar">
        </activity>
        <activity
            android:name=".BezeroaSortu"
            android:theme="@style/Theme.NewTelApp.NoActionBar">
        </activity>
        <activity
            android:name=".AurrekontuaMenua"
            android:theme="@style/Theme.NewTelApp.NoActionBar">
        </activity>
        <activity
            android:name=".AurrekontuakIkusi"
